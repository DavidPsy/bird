package com.joinus.server.cache;

import java.io.Serializable;
import java.util.List;

/**
 * 活动缓存<br>
 * @author henrybit
 * @version 1.0
 * @since 2013-4-17
 */
public class MeetingCache implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 52097346333885897L;
	/**活动id*/
	protected int meetingId;
	/**活动名*/
	protected String name;
	/**活动类型 1-购物,2-运动,3-会议,4-聚餐,5-游戏*/
	protected int tag;
	/**活动描述*/
	protected String content;
	/**活动发起者*/
	protected String createUserId;
	/**活动参加的人员列表*/
	protected List<String> joinUsersId;
	/**活动当前状态*/
	protected int status;
	/**活动的地址*/
	protected String address;
	/**活动地址中文描述*/
	protected String addressDescription;
	/**活动的时间*/
	protected long startTime;
	
	
}
